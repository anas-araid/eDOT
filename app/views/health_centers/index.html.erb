<h1>Health Centers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_centers.each do |health_center| %>
      <tr>
        <td><%= health_center.name %></td>
        <td><%= health_center.address %></td>
        <td><%= health_center.phone %></td>
        <td><%= link_to 'Show', health_center %></td>
        <td><%= link_to 'Edit', edit_health_center_path(health_center) %></td>
        <td><%= link_to 'Destroy', health_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health Center', new_health_center_path %>
